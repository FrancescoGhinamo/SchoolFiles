package ricettario;

public class Ricettario {
	
	private static final int MAX_RICETTE = 100;
	
	private Ricetta[] ricette;
	
	private int primoIndiceLibero() {
		int i = 0;
		for(i = 0; (i < ricette.length) && (ricette[i] == null); i++) {
			
		}
		if(i >= ricette.length) {
			i = ricette.length - 1;
		}
		
		return i;
	}
	
	
	
}
